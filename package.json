{
  "name": "tweed.js",
  "version": "1.0.0",
  "description": "Simple web framework",
  "type": "module",
  "module": "lib/esm/index.esm.js",
  "license": "MIT",
  "homepage": "https://github.com/ff34d/tweed#readme",
  "main": "lib/esm/index.esm.js",
  "types": "types/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "lib/esm",
    "lib/cjs"
  ],
  "keywords": [
    "tweed"
  ],
  "scripts": {
    "format": "prettier --write",
    "lint": "eslint --fix",
    "test": "jest",
    "build": "rm -rf ./dist && rollup --config",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
